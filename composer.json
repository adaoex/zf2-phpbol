{
    "name": "adaoex/zf2-phpbol",
    "description": "ZF2-PHPBol Geração de boletos com ZF2",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2"
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.2.*",
        "dino/dompdf-module": "*",
        "twig/twig": "1.*",
        "twig/extensions": "dev-master",
        "data-uri/data-uri": "dev-master",
        "data-uri/twig-extension": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Twig_Extensions_": "lib/",
            "DataURI": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    }
}
